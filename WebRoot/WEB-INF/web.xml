<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <!-- 用户登陆-->
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.applyroom.servlet.LoginServlet</servlet-class>
    </servlet>
<!-- Servlet映射 -->
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <!--学生注册 -->
     <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>com.applyroom.servlet.RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ListTeacherByCampus</servlet-name>
        <servlet-class>com.applyroom.servlet.ListTeacherByCampus</servlet-class>
    </servlet>
    <servlet-mapping>
      <servlet-name>ListTeacherByCampus</servlet-name>
      <url-pattern>/ListTeacherByCampus</url-pattern>
    </servlet-mapping>
</web-app>
